#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;i<=9;i++)
        printf("%3d",i);
    printf("\n");
    printf("  -  -  -  -  -  -  -  -  -");
    printf("\n");
    for(j=1;j<=9;j++)
    {
      for(i=1;i<=9;i++)
      {
          if(i<j)
            printf("   ");
          if(i>=j)
          printf("%3d",i*j);
      }
      printf("\n");
    }
    return 0;
}

