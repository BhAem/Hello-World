#include<stdio.h>
int main()
{
   int i,j;
   for(i=1;i<=9;i++)
        printf("%3d",i);
    printf("\n");
    printf("  -  -  -  -  -  -  -  -  -");
    printf("\n");
   for(j=1;j<=9;j++)
    {
      for(i=1;i<=9;i++)
      {
          if(j<i)
            printf("  ");
          if(j>=i)
          printf("%3d",j*i);
      }
      printf("\n");
    }
    return 0;
}
