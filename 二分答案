//洛谷P2440 木材加工

//常规做法，风险大
#include<bits/stdc++.h>
using namespace std;
int n,k,ans,a[100005],l=0,r=100000001,mid;
int main()
{
	cin>>n>>k;
	for(int i=1;i<=n;i++)
		cin>>a[i];
	while(l<=r)
	{
		mid=(l+r)/2;
		int cnt=0;
		for(int i=1;i<=n;i++)
			cnt+=a[i]/mid;
		if(cnt>=k)
		{
			ans=mid;
			l=mid+1;
		}
		else r=mid-1;
	}
	cout<<ans;
	return 0;
}

//改良办法
#include<bits/stdc++.h>
using namespace std;
int n,k，a[100005],l=0,r=100000001,mid;
int main()
{
	cin>>n>>k;
	for(int i=1;i<=n;i++)
	cin>>a[i];
	while(l+1<r)
	{
		mid=(l+r)/2;
		int cnt=0;
		for(int i=1;i<=n;i++)
		cnt+=a[i]/mid;
		if(cnt>=k)l=mid;
		else r=mid;
	}
	cout<<l;
}
